            android:name="com.example.bdcentros.Pantalla_BDCentros"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.bdcentros.Pantalla_centros"
            android:label="@string/pant_centros"></activity>
        <activity android:name="com.example.bdcentros.Pantalla_personal" 
            android:label="@string/pant_personal"></activity>
        <activity android:name="com.example.bdcentros.Pantalla_profesores"
            android:label="@string/pant_profesores"></activity>
    </application>

</manifest>
